<h1>All Users</h1>
<table class="w3-table w3-striped w3-table-all">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Admin</th>
  </tr>
  <% @users.each do |u| %>
  <tr>
    <td><%= link_to u.name, user_path(u) %></td>
    <td><%= u.email %></td>
    <td><%= u.is_admin? %></td>
  </tr>
  <% end %>
</table>
