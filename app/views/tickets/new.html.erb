<%= form_for @ticket do |f| %>
  <%= f.hidden_field :creator_id, :value => current_user.id %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :status_id %><br>
  <%= f.collection_select :status_id, Status.all, :id, :name %><br>
  <%= f.label :assignee_id %><br>
  <%= f.collection_select :assignee_id, User.all, :id, :name %><br>
  <%= f.label :due_date %><br>
  <%= f.date_field :due_date %><br><br>
  <%= f.submit %><br>
<% end %>
